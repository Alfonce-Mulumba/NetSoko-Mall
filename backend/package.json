{
  "name": "net-soko-backend",
  "version": "1.0.0",
  "description": "Net-Soko Marketplace Backend",
  "main": "src/server.js",
  "type": "module",
  "scripts": {
    "clean": "rimraf prisma/generated",
    "prebuild": "node -e \"try{require('fs').chmodSync('./node_modules/.bin/prisma', 0o755)}catch(e){}\"",
    "build": "npm run prebuild && npx prisma generate",
    "start": "node src/server.js",
    "dev": "nodemon src/server.js"
  },
  "dependencies": {
    "@prisma/client": "^6.18.0",
    "bcryptjs": "^2.4.3",
    "cloudinary": "^1.41.3",
    "cors": "^2.8.5",
    "csv-parser": "^3.2.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "express-rate-limit": "^8.1.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^7.0.0",
    "multer": "^2.0.2",
    "multer-storage-cloudinary": "^4.0.0",
    "nodemailer": "^7.0.6",
    "pg": "^8.16.3",
    "pino": "^9.11.0",
    "pino-multi-stream": "^3.1.2",
    "pino-pretty": "^13.1.1",
    "prisma": "^6.18.0",
    "react-toastify": "^11.0.5",
    "streamifier": "^0.1.1"
  },
  "devDependencies": {
